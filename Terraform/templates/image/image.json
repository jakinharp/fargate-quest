[
    {
      "name": "rearcQuestApp",
      "image": "${app_image}",
      "cpu": ${fargate_cpu},
      "memory": ${fargate_memory},
      "networkMode": "awsvpc",
      "logConfiguration": {
          "logDriver": "awslogs",
          "options": {
            "awslogs-group": "/ecs/rearcQuestApp",
            "awslogs-region": "${aws}",
            "awslogs-stream-prefix": "ecs"
          }
      },
      "portMappings": [
        {
          "containerPort": ${app_port},
          "hostPort": ${app_port}
        }
      ],
      "environment": [
        {
          "name": "SECRET_WORD",
          "value" "TwelveFactor"
        }
      ]
    }
  ]